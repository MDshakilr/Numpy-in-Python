# create numpy array
import numpy as np
x = np.array([1,2,3])
print(x)
print(type(x))

#list
x=[1,2,3,4]
print(x)
print(type(x))



#faster numpy




import numpy as np
import time

start_time = time.time()
for _ in range(10000):
    np.arange(1, 9)**4
end_time = time.time()

execution_time = end_time - start_time
print(f"Execution time: {execution_time:.5f} seconds for 10,000 iterations")



